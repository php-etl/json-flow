{
    "name": "php-etl/json-flow",
    "description": "JSON implementation for ETL Pipeline",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "php-etl/pipeline-contracts": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "adlawson/vfs": "*",
        "php-etl/phpunit-extension": "^0.2.0"
    },
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Flow\\JSON\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\Flow\\JSON\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
