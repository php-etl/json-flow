{
    "name": "php-etl/json-flow",
    "description": "JSON implementation for ETL Pipeline",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "php-etl/pipeline-contracts": "0.5.*",
        "php-etl/bucket": "*",
        "php-etl/bucket-contracts": "0.3.*",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mikey179/vfsstream": "^1.6",
        "rector/rector": "^0.15",
        "php-etl/phpunit-extension": "0.7.*"
    },
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Flow\\JSON\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\Flow\\JSON\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    }
}
